<h1>Animals#new</h1>
<p>Find me in app/views/animals/new.html.erb</p>
<%= simple_form_for(@animal) do |f| %>
  <div>
    <%= f.input :name %>
  </div>

  <div>
    <%= f.input :species %>
  </div>

  <div>
    <%= f.input :age %>
  </div>

  <div>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
  </div>

  <div>
    <%= f.button :submit %>
  </div>
<% end %>
